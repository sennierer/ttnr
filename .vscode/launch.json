    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "--settings=apis.settings.server"
            ],
            "django": true
        },
        {
            "name": "Remote Jupyter",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "shell_plus",
                "--notebook",
                "--settings=apis.settings.server"
            ],
            "django": true
        },
    ]
}